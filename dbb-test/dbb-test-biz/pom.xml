<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>dbb-test</artifactId>
        <groupId>top.dabaibai</groupId>
        <version>1.0.0</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>dbb-test-biz</artifactId>

    <dependencies>
        <!--test模块api-->
        <dependency>
            <groupId>top.dabaibai</groupId>
            <artifactId>dbb-test-api</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>top.dabaibai</groupId>
            <artifactId>dbb-user-api</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>top.dabaibai</groupId>
            <artifactId>dbb-web-starter</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>top.dabaibai</groupId>
            <artifactId>dbb-seata</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>top.dabaibai</groupId>
            <artifactId>dbb-stream</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>top.dabaibai</groupId>
            <artifactId>dbb-grpc-api</artifactId>
            <exclusions>
                <exclusion>
                    <groupId>net.devh</groupId>
                    <artifactId>grpc-server-spring-boot-starter</artifactId>
                </exclusion>
            </exclusions>
            <version>1.0.0</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <version>${spring-boot.version}</version>
                <configuration>
                    <excludes>
                        <exclude>
                            <groupId>org.projectlombok</groupId>
                            <artifactId>lombok</artifactId>
                        </exclude>
                    </excludes>
                </configuration>
                <executions>
                    <execution>
                        <goals>
                            <goal>repackage</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>